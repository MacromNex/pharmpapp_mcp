{
  "project_name": "cycpep-tools",
  "description": "MCP tools for cyclic peptide computational analysis - Permeability prediction and molecular descriptor calculation",
  "mcp_server_name": "cyclic-peptide-tools",
  "total_tools": 13,
  "tools": [
    {
      "name": "get_job_status",
      "category": "job_management",
      "type": "sync",
      "description": "Check job progress and status",
      "example_prompt": "Check the status of job abc12345"
    },
    {
      "name": "get_job_result",
      "category": "job_management",
      "type": "sync",
      "description": "Get completed job results",
      "example_prompt": "Get the results for job abc12345"
    },
    {
      "name": "get_job_log",
      "category": "job_management",
      "type": "sync",
      "description": "View job execution logs",
      "example_prompt": "Get the logs for job abc12345"
    },
    {
      "name": "cancel_job",
      "category": "job_management",
      "type": "sync",
      "description": "Cancel running job",
      "example_prompt": "Cancel job abc12345"
    },
    {
      "name": "list_jobs",
      "category": "job_management",
      "type": "sync",
      "description": "List all jobs with status filter",
      "example_prompt": "List all my jobs"
    },
    {
      "name": "cleanup_old_jobs",
      "category": "job_management",
      "type": "sync",
      "description": "Remove old completed jobs",
      "example_prompt": "Clean up jobs older than 30 days"
    },
    {
      "name": "calculate_cyclic_peptide_descriptors",
      "category": "analysis",
      "type": "sync",
      "description": "Calculate molecular descriptors for cyclic peptides using RDKit",
      "example_prompt": "Calculate descriptors for @examples/data/demo_cyclic_peptides.smi"
    },
    {
      "name": "predict_cyclic_peptide_permeability",
      "category": "analysis",
      "type": "sync",
      "description": "Predict permeability with single model",
      "example_prompt": "Predict permeability using caco2_c model for @examples/data/\"example for Caco2_C.csv\""
    },
    {
      "name": "submit_batch_analysis",
      "category": "analysis",
      "type": "submit",
      "description": "Multi-model comparison analysis",
      "example_prompt": "Submit batch analysis for @descriptors.csv using models caco2_c and rrck_c"
    },
    {
      "name": "submit_large_descriptor_calculation",
      "category": "analysis",
      "type": "submit",
      "description": "Large-scale descriptor calculation",
      "example_prompt": "Submit large descriptor calculation for @large_dataset.smi with 3D descriptors"
    },
    {
      "name": "get_available_models",
      "category": "utility",
      "type": "sync",
      "description": "List available permeability models",
      "example_prompt": "What permeability models are available?"
    },
    {
      "name": "validate_input_file",
      "category": "utility",
      "type": "sync",
      "description": "Validate SMILES/SDF file",
      "example_prompt": "Validate the input file @examples/data/demo_cyclic_peptides.smi"
    },
    {
      "name": "get_example_data",
      "category": "utility",
      "type": "sync",
      "description": "List available example data",
      "example_prompt": "What example data is available for testing?"
    }
  ],
  "scripts": [
    {
      "name": "calculate_descriptors.py",
      "path": "scripts/calculate_descriptors.py",
      "description": "Calculate molecular descriptors from SMILES/SDF files",
      "example_command": "python scripts/calculate_descriptors.py --input examples/data/demo_cyclic_peptides.smi --output descriptors.csv"
    },
    {
      "name": "predict_permeability.py",
      "path": "scripts/predict_permeability.py",
      "description": "Predict permeability using PharmPapp models",
      "example_command": "python scripts/predict_permeability.py --input descriptors.csv --model caco2_c --output predictions.csv"
    },
    {
      "name": "batch_analysis.py",
      "path": "scripts/batch_analysis.py",
      "description": "Batch analysis and model comparison",
      "example_command": "python scripts/batch_analysis.py --input descriptors.csv --output batch_results/ --models caco2_c rrck_c"
    }
  ],
